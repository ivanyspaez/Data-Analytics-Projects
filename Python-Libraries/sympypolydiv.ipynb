      "source": [
        "from sympy import Symbol\n",
        "from sympy import div\n",
        "\n",
        "x = Symbol('x')\n",
        "\n",
        "def Poly(p,mod):\n",
        "    q,r = div(p,mod,x) #quotient and remainder polynomial division by modulus mod\n",
        "    return q.as_poly(x,domain='GF(2)') #Z_2\n",
        "\n",
        "p = x**5 - 2*x**4  - x**3 + 8*x**2 - 10*x - 4\n",
        "m = x- -1\n",
        "print(Poly(p,m))"
      ],
      "execution_count": 1,
